{
      "name": "Neo Test",
      "version": "1.0.0",
      "guard": "-",
      "group": "neo",
      "paths": [
            {
                  "path": "/chat",
                  "method": "GET",
                  "process": "scripts.neo.chat",
                  "in": [
                        "$query.content",
                        "$query.context",
                        "$query.token"
                  ],
                  "out": {
                        "status": 200,
                        "type": "application/json",
                        "stream": true
                  }
            }
      ]
}